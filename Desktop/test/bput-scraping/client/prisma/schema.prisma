generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL") // replace this with your Neon DB connection string
}

model Roland {
  id Int @id @default(autoincrement())
  studentName String
  rollNo  String
  sgpa String
  branch String
  subjects String[]
  semester Int
}
